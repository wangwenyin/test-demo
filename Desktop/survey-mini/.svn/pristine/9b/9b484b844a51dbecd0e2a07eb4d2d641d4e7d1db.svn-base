        </view>
        <view class='item'>
          <view>主要交通干线(名称)</view>
          <input type="text" name='zyjtgx_mc' placeholder="请输入名称" value='{{form.zyjtgx_mc}}' />
        </view>
        <view class='item'>
          <view>主要交通干线(距离)</view>
          <input type="number" name='zyjtgx_jl' placeholder="请输入距离" value='{{form.zyjtgx_jl}}' />
        </view>
        <view class='title'>公共设施</view>
