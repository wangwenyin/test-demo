      <view class='title'>装修情况</view>
      <view class='sub_title'>1.基本状况</view>
      <view class='box'>
        <view class='item'>
          <view>外墙 </view>
          <radio-group name='zxqk_wq' data-name="wq" bindchange="radioChange">
            <label class="radio" wx:key="{{index}}" wx:for="{{outerWalls}}">
