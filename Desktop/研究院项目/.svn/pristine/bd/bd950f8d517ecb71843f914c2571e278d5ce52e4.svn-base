<script>
import NormalityChart from '@/views/resultaudit/components/NormalityChart'
import { getCaseShow
} from '@/api/ratio'
export default {
  components: {
    NormalityChart
  },
  props: {
    form: {
      type: Object,
      default: null
    },
    detail: {
      type: Boolean,
      default: false
    },
    width: {
      type: String,
      default: '100%'
    },
    'histogram-data': {
      type: Array,
      default: []
    }
  },
  data() {
    return {
      caseAllList: [],
      currentpage: 1,
      pagesize: 10
    }
  },
  methods: {
    view() {
      const params = { offset: 0 } // usage1: 总体情况
      getCaseShow(this.form.id, params).then(response => {
        this.caseAllList = response.data
      })
    },
    save(method, item, comment) {
      var params = {
        method: method,
        item: item,
        comment: comment
      }
      this.$emit('save', params)
    },
    handleSizeChange(size) {
      this.pageSize = size
    },
    handleCurrentChange(val) {
      this.currentpage = { val }.val
    },
    typeIndex(index) {
      return index + (this.currentpage - 1) * this.pagesize + 1
    }
  }
}
</script>
